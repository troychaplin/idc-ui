{
  "name": "@troychaplin79/idc-ui",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "main": "dist/idc-ui.cjs.js",
  "module": "dist/idc-ui.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "/dist/index.css"
  ],
  "scripts": {
    "build": "tsc && yarn format && yarn lint && vite build && yarn build:css && yarn size",
    "build:css": "tailwindcss --postcss -m -i ./src/tailwind.css -o ./dist/index.css",
    "storybook": "concurrently \"yarn storybook:css\" \"storybook dev -p 6006\"",
    "storybook:css": "tailwindcss --postcss -w -i ./src/tailwind.css -o ./.storybook/index.css",
    "build-storybook": "concurrently \"yarn build-storybook:css\" \"storybook build\"",
    "build-storybook:css": "tailwindcss --postcss -m -i ./src/tailwind.css -o ./.storybook/index.css",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier -w src",
    "size": "size-limit",
    "clean": "rm -rf node_modules ; rm yarn.lock ; yarn install"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@size-limit/esbuild": "^8.2.4",
    "@size-limit/esbuild-why": "^8.2.4",
    "@size-limit/file": "^8.2.4",
    "@storybook/addon-a11y": "^7.0.0-rc.4",
    "@storybook/addon-essentials": "^7.0.0-rc.4",
    "@storybook/addon-interactions": "^7.0.0-rc.4",
    "@storybook/addon-links": "^7.0.0-rc.4",
    "@storybook/blocks": "^7.0.0-rc.4",
    "@storybook/react": "^7.0.0-rc.4",
    "@storybook/react-vite": "^7.0.0-rc.4",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.2.4",
    "storybook": "^7.0.0-rc.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0",
    "vite-tsconfig-paths": "^4.0.7"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
